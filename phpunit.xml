<?xml version="1.0" encoding="UTF-8"?>
<phpunit xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" colors="true" xsi:noNamespaceSchemaLocation="https://schema.phpunit.de/10.0/phpunit.xsd" cacheDirectory=".phpunit.cache">
  <coverage>
    <include>
      <directory suffix=".php">./src</directory>
    </include>
    <report>
      <html outputDirectory="./build/coverage-html" lowUpperBound="35" highLowerBound="75"/>
    </report>
  </coverage>
  <testsuites>
    <testsuite name="Telegram Error Report Test Suite">
      <directory>./test</directory>
    </testsuite>
  </testsuites>
  <logging/>
</phpunit>
